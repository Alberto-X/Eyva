  */
function evaluateProperty({api, helpers}) {
  if (!api.state.active_worker) {
    return '[No active worker]';
  }
    var activeShiftWorker = api.data.look_up_shift_workers.results.find(sw => {
    return sw.worker.value == api.state.active_worker.id;
  });
	return activeShiftWorker.worker.displayValue;
}]]></script>
